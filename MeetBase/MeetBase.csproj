<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>


  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="7.0.10" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Atom">
      <HintPath>..\Atom.dll</HintPath>
    </Reference>
    <Reference Include="Atom.Blazor">
      <HintPath>..\Atom.Blazor.dll</HintPath>
    </Reference>
    <Reference Include="Atom.Blazor.Controls">
      <HintPath>..\Atom.Blazor.Controls.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
